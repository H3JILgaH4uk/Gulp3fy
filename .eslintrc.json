{
    "extends": "airbnb-base",
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true
        // "jquery": true
        // "es2021": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "linebreak-style": "off",
        "eol-last": "off",
        "no-unused-vars": "off",
        "no-trailing-spaces": "off",
        // "comma-dangle": ["error", "never"],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": ["gulpfile.babel.js", "gulp/**/*"]}]
    }
}
