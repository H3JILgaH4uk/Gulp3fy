{
	"name": "gulp3fy",
	"version": "0.2.0",
	"description": "Gulp TaskManager for Front-End",
	"type": "module",
	"scripts": {
		"dev": "gulp dev",
		"build": "gulp prod --prod",
		"zip": "gulp zip --prod",
		"deploy": "gulp deploy --prod",
		"sprite": "gulp spritesBuild",
		"eslint": "eslint src/scripts --ext .js",
		"eslint:fix": "npm run eslint -- --fix",
		"stylelint": "stylelint src/styles .scss",
		"stylelint:fix": "npm run stylelint -- --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/H3JILgaH4uk/Gulp3fy.git"
	},
	"keywords": [],
	"author": "H3JILgaH4uk",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/H3JILgaH4uk/Gulp3fy/issues"
	},
	"homepage": "https://github.com/H3JILgaH4uk/Gulp3fy#readme",
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead",
		"not IE 11"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged --relative"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix"
		],
		"*.(css|scss)": [
			"stylelint --fix"
		]
	},
	"dependencies": {
		"overlayscrollbars": "^1.13.1",
		"vanilla-lazyload": "^17.5.0"
	},
	"devDependencies": {
		"@babel/core": "^7.16.0",
		"@babel/eslint-parser": "^7.16.3",
		"@babel/preset-env": "^7.16.0",
		"@babel/register": "^7.16.0",
		"@zoxon/emitty": "^1.4.2",
		"babelify": "^10.0.0",
		"browser-sync": "^2.27.7",
		"browserify": "^17.0.0",
		"del": "^6.0.0",
		"eslint": "^8.2.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-changed": "^4.0.3",
		"gulp-clean-css": "^4.3.0",
		"gulp-group-css-media-queries": "^1.2.2",
		"gulp-if": "^3.0.0",
		"gulp-imagemin": "^8.0.0",
		"gulp-plumber": "^1.2.1",
		"gulp-pretty-html": "^2.0.10",
		"gulp-pug": "^5.0.0",
		"gulp-rename": "^2.0.0",
		"gulp-sass": "^5.0.0",
		"gulp-sass-glob": "^1.1.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-svg-sprite": "^1.5.0",
		"gulp-ttf2woff2": "^4.0.1",
		"gulp-uglify": "^3.0.2",
		"gulp-webpcss": "^1.1.1",
		"husky": "^7.0.4",
		"imagemin-pngquant": "^9.0.2",
		"imagemin-webp": "^6.0.0",
		"import-fresh": "^3.3.0",
		"lint-staged": "^11.2.6",
		"npm-check-updates": "^12.0.2",
		"prettier": "^2.4.1",
		"pug-include-glob": "^0.4.3",
		"pug-lint": "^2.6.0",
		"sass": "^1.43.4",
		"stylelint": "^14.0.1",
		"stylelint-config-idiomatic-order": "^8.1.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard": "^23.0.0",
		"stylelint-config-standard-scss": "^2.0.1",
		"stylelint-order": "^5.0.0",
		"stylelint-prettier": "^2.0.0",
		"stylelint-scss": "^4.0.0",
		"vinyl-buffer": "^1.0.1",
		"vinyl-source-stream": "^2.0.0"
	}
}
