	"main": "gulpfile.js",
	"type": "module",
	"scripts": {
		"dev": "gulp dev",
		"prod": "gulp prod --prod",
		"preview": "gulp dev --prod",
		"zip": "gulp archiving --prod",
		"favicon": "gulp faviconsBuild",
